<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="form.service.contributo.solidarieta.model.Donatore" />
	<class name="form.service.contributo.solidarieta.model.impl.DonatoreImpl" table="CNTB_SOLID_DONATORE_Donatore">
		<id column="id_" name="id" type="long">
			<generator class="sequence">
				<param name="sequence">donatore_seq</param>
			</generator>
		</id>
		<property name="nome" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="cognome" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="cod_fiscale" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="importo" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="data_inserimento" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>